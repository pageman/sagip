                center: new L.LatLng(13.00,121.41),
                zoom: 5,
                minZoom: 5,
                crs: L.CRS.EPSG4326,
                maxZoom: 18,
                attributionControl:false,
                layers: [stamen],
                zoomControl: false
        });
        
        
